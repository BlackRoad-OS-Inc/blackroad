tmux new-window -t $SESSION -n "â˜ï¸ Cloud"
tmux split-window -h -t $SESSION
tmux send-keys -t $SESSION:17.0 'ssh shellfish' Enter
tmux send-keys -t $SESSION:17.1 'ssh blackroad os-infinity' Enter

