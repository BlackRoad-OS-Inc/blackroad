set -euo pipefail

ROOTS="${QWEN_AGENT_ROOTS:-/Users/alexa}"
MODEL="${QWEN_AGENT_MODEL:-qwen2.5-coder:7b}"

python3 /Users/alexa/tools/qwen-coder-assistant/assistant.py \
  --root "$ROOTS" \
  --model "$MODEL" \
  --auto-approve \
  "$@"
